    (2*(-4 + d - n5 - n6)) + j[v36LRpp, 2, 1, 0, 1, n5, n6, 1, 1, 1]/
    (-4 + d - n5 - n6) - (mQ^2*\[Gamma]*j[v36LRpp, 2, 1, 1, 1, n5, 1 + n6, 1, 
      0, 1])/(-4 + d - n5 - n6), 
 j[v36LRpp, (n1_)?Positive, (n2_)?Positive, (n3_)?Positive, (n4_)?Positive, 
    (n5_)?NonPositive, (n6_)?NonPositive, (n7_)?Positive, (n8_)?Positive, 
    (n9_)?Positive] /; n1 == 1 && n2 == 1 && n3 == 1 && n4 == 1 && n6 == 0 && 
    n7 == 1 && n8 == 1 && n9 == 1 && n5 != 0 -> 
  (n5*j[v36LRpp, 0, 1, 1, 1, 1 + n5, 0, 1, 1, 1])/(2*(-4 + d - n5)) + 
