   (n5*j[v36LRpp, 1, 0, 1, 1, 1 + n5, 0, 1, 1, 1])/(2*(-4 + d - n5)) + 
   (mQ^2*j[v36LRpp, 1, 0, 2, 1, 1 + n5, 0, 1, 1, 1])/(2*(-4 + d - n5)) + 
   (n5*j[v36LRpp, 1, 1, 0, 1, 1 + n5, 0, 1, 1, 1])/(2*(-4 + d - n5)) - 
   (n5*j[v36LRpp, 1, 1, 1, 0, 1 + n5, 0, 1, 1, 1])/(2*(-4 + d - n5)) + 
   (mQ^2*j[v36LRpp, 1, 1, 1, 1, 1 + n5, 0, 0, 2, 1])/(2*(-4 + d - n5)) + 
